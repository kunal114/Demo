| makeresults count=1
| eval url="your_aws_health_url"
| rest method=GET url=$url$
| eval check_time=_time
| stats latest(status) as status latest(check_time) as last_check
| eval status_message=case(
    status=200, "Healthy",
    status=500, "Server Error",
    status=503, "Service Unavailable",
    status=404, "Not Found",
    true(), "Unknown Status"
)
