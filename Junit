import java.lang.reflect.Method;

class YourFilterTest {
    private YourFilter filter;
    private Method doFilterInternalMethod;

    @BeforeEach
    void setUp() throws NoSuchMethodException {
        filter = new YourFilter();
        doFilterInternalMethod = YourFilter.class.getDeclaredMethod("doFilterInternal", 
            HttpServletRequest.class, HttpServletResponse.class, FilterChain.class);
        doFilterInternalMethod.setAccessible(true);
    }

    @Test
    void testDoFilterInternal_NonMultipartRequest() throws Exception {
        MockHttpServletRequest request = new MockHttpServletRequest();
        MockHttpServletResponse response = new MockHttpServletResponse();

        doFilterInternalMethod.invoke(filter, request, response, filterChain);

        verify(filterChain).doFilter(request, response);
    }

    // Other test methods follow a similar pattern
}
