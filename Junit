import static org.junit.jupiter.api.Assertions.assertNotNull;
import static org.junit.jupiter.api.Assertions.assertEquals;

import org.junit.jupiter.api.Test;
import org.springframework.mock.web.MockFilterConfig;
import org.springframework.web.filter.Filter;

import net.jpmchase.pbcf.dashboardmgr.FilterConfig;

class FilterConfigTest {

    @Test
    void testEmulationFilter() {
        // Arrange
        FilterConfig filterConfig = new FilterConfig();
        MockFilterConfig mockFilterConfig = new MockFilterConfig();

        // Act
        Filter filter = filterConfig.emulationFilter().getFilter();
        filter.init(mockFilterConfig);

        // Assert
        assertNotNull(filter, "Filter should not be null");
        assertEquals(1, filterConfig.emulationFilter().getOrder(), "Filter order should be 1");
    }
}
